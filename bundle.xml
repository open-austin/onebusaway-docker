<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <bean id="gtfs-bundles" class="org.onebusaway.transit_data_federation.bundle.model.GtfsBundles">
        <property name="bundles">
            <list>
                <ref bean="gtfsMeow" />
            </list>
        </property>
    </bean>
    <bean id="gtfsMeow" class="org.onebusaway.transit_data_federation.bundle.model.GtfsBundle">
        <property name="path" value="/gtfs.zip" />
    </bean>
</beans>
